<block wx:for="{{leftList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toUrl',['/pages/goodDetails/goodDetails?id='+item.id]]]]]}}" class="demo-warter pLeft data-v-5f0ad9dd" bindtap="__e"><view class="demo-warter-hide data-v-5f0ad9dd"><u-lazy-load vue-id="{{'c25faf7e-5-'+i+'-'+index}}" threshold="-450" image="{{item.goodsImageUrl||'../../../static/logo.png'}}" index="{{index}}" class="data-v-5f0ad9dd" bind:__l="__l"></u-lazy-load></view><view class="demo-title data-v-5f0ad9dd">{{''+(item.goodsName||'哥窝里')+''}}</view><block wx:if="{{item.shop}}"><view class="demo-shop data-v-5f0ad9dd">{{''+(item.shop||'')+''}}</view></block><view class="demo-price data-v-5f0ad9dd"><text class="red data-v-5f0ad9dd">￥</text><text class="red price data-v-5f0ad9dd">{{item.goodsCurrentPrice||0}}</text><block wx:if="{{item.isScoreBuy==1}}"><text class="red data-v-5f0ad9dd">{{"/"+item.scoreSum+"积分"}}</text></block><text class="orPrice data-v-5f0ad9dd">{{"￥"+(item.goodsOriginalPrice||0)}}</text></view><view class="giftScore data-v-5f0ad9dd">{{"可获取"+item.giftScore+"积分"}}</view></view></block>